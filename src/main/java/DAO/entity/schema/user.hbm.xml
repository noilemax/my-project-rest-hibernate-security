<hibernate-mapping>
<class name="DAO.entity.User" table="users">
    <id column="ID" name="userId" type="java.lang.Long">
        <generator class="increment" />
    </id>
<property column="userName" name="userName" type="java.lang.String"/>
<property column="password" name="passWord" type="java.lang.String"/>

    <set name="userroles" table="roles" lazy="false">
    <key column="ID"/>
        <many-to-many column="ID" class="DAO.entity.Role"/>
    </set>
    </class>
</hibernate-mapping>